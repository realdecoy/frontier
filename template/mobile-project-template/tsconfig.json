{
  "compilerOptions": {
    "resolveJsonModule": true,
    "moduleResolution": "node",  
      "types": [
          "node",
          "webdriverio/async",
          "@wdio/mocha-framework",
          "mocha",
          "expect-webdriverio"
      ],
      "target": "ES5",
      "baseUrl": "src",
      "paths": {
      "@/page_objects/*": [
        "page_objects/*"
      ],
      "@/screen_objects/*": [
